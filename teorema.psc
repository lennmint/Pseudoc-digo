Proceso teorema
	
	//Definir Variables: x1, x2, y1, y2, res, raiz
	Definir x1, x2, y1, y2, resultado, distancia Como Real;
	
	Escribir "Ingrese valores: x1, x2, y1, y2 en ese orden: ";
	Leer x1, x2, y1, y2;
	
	resultado <-  (x1 - x2)^2 + (y1 - y2)^2;
	distancia <- raiz (resultado);
	
	Escribir "Distancia: ", distancia;
	
FinProceso
